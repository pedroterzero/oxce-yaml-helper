manufacture:
  - name: STR_DUMMY_MANUFACTURE
    requiredItems:
      STR_ITEM_DELETE_TEST: 1
    randomProducedItems:
    - [25, {STR_DUMMY_NESTED_ITEM: 1}]

  - name: STR_MANUFACTURE_PRODUCTED_ITEMS_ITEMS_TEST
    producedItems:
      STR_DUMMY_ITEM: 1
  - name: STR_MANUFACTURE_PRODUCTED_ITEMS_CRAFTS_TEST
    producedItems:
      STR_DUMMY_CRAFT: 1
  # missing type
  - name: STR_MANUFACTURE_PRODUCTED_ITEMS_TEST_MISSING
    producedItems:
      STR_DUMMY_NON_EXISTING_REFERENCE: 1
  # incorrect type
  - name: STR_MANUFACTURE_PRODUCTED_ITEMS_TEST_INCORRECT
    producedItems:
      STR_DUMMY_RESEARCH: 1

  - name: STR_MANUFACTURE_RANDOM_PRODUCTED_ITEMS_ITEMS_TEST
    randomProducedItems:
      - [25, {STR_DUMMY_ITEM: 1}]
  - name: STR_MANUFACTURE_RANDOM_PRODUCTED_ITEMS_CRAFTS_TEST
    randomProducedItems:
      - [25, {STR_DUMMY_CRAFT: 1}]
  # missing type
  - name: STR_MANUFACTURE_RANDOM_PRODUCTED_ITEMS_TEST_MISSING
    randomProducedItems:
      - [25, {STR_DUMMY_NON_EXISTING_REFERENCE: 1}]
  # incorrect type
  - name: STR_MANUFACTURE_RANDOM_PRODUCTED_ITEMS_TEST_INCORRECT
    randomProducedItems:
      - [25, {STR_DUMMY_RESEARCH: 1}]

  - name: STR_MANUFACTURE_REQUIRED_ITEMS_ITEMS_TEST
    requiredItems: {STR_DUMMY_ITEM: 1}
  - name: STR_MANUFACTURE_REQUIRED_ITEMS_CRAFTS_TEST
    requiredItems: {STR_DUMMY_CRAFT: 1}
  # missing type
  - name: STR_MANUFACTURE_REQUIRED_ITEMS_TEST_MISSING
    requiredItems: {STR_DUMMY_NON_EXISTING_REFERENCE: 1}
  # incorrect type
  - name: STR_MANUFACTURE_REQUIRED_ITEMS_TEST_INCORRECT
    requiredItems: {STR_DUMMY_RESEARCH: 1}

  - name: STR_MANUFACTURE_REQUIRES_RESEARCH_TEST
    requires: [STR_DUMMY_RESEARCH]
  # missing type
  - name: STR_MANUFACTURE_REQUIRES_TEST_MISSING
    requires: [STR_DUMMY_NON_EXISTING_REFERENCE]
  # incorrect
  - name: STR_MANUFACTURE_REQUIRES_TEST_INCORRECT
    requires: [STR_DUMMY_ITEM]

  - name: STR_MANUFACTURE_REQUIRES_BASE_FUNC_TEST
    requiresBaseFunc: [ STR_DUMMY_BASE_FUNC ]
  # missing type
  - name: STR_MANUFACTURE_REQUIRES_BASE_FUNC_TEST_MISSING
    requiresBaseFunc: [ STR_DUMMY_NON_EXISTING_REFERENCE ]
  # incorrect
  - name: STR_MANUFACTURE_REQUIRES_BASE_FUNC_TEST_INCORRECT
    requiresBaseFunc: [ STR_DUMMY_RESEARCH ]

  - name: STR_MANUFACTURE_SPAWNED_PERSON_TYPE_SOLDIERS_TEST
    spawnedPersonType: STR_DUMMY_SOLDIER
  - name: STR_MANUFACTURE_SPAWNED_PERSON_TYPE_SCIENTISTS_TEST
    spawnedPersonType: STR_SCIENTIST
  - name: STR_MANUFACTURE_SPAWNED_PERSON_TYPE_ENGINEERS_TEST
    spawnedPersonType: STR_ENGINEER
  # missing type
  - name: STR_MANUFACTURE_SPAWNED_PERSON_TYPE_TEST_MISSING
    spawnedPersonType: STR_DUMMY_NON_EXISTING_REFERENCE
  # incorrect
  - name: STR_MANUFACTURE_SPAWNED_PERSON_TYPE_TEST_INCORRECT
    spawnedPersonType: STR_DUMMY_RESEARCH
