manufactureShortcut:
  - name: STR_MANUFACTURE_SHORTCUT_WITHOUT_STARTFROM

  # type validations

  - name: STR_MANUFACTURE_SHORTCUT_BREAKDOWNITEMS_TEST
    startFrom: STR_DUMMY_MANUFACTURE
    breakDownItems: [ STR_DUMMY_MANUFACTURE ]
  # missing type
  - name: STR_MANUFACTURE_SHORTCUT_BREAKDOWNITEMS_TEST_MISSING
    startFrom: STR_DUMMY_MANUFACTURE
    breakDownItems: [ STR_DUMMY_NON_EXISTING_REFERENCE ]
  # incorrect
  - name: STR_MANUFACTURE_SHORTCUT_BREAKDOWNITEMS_TEST_INCORRECT
    startFrom: STR_DUMMY_MANUFACTURE
    breakDownItems: [ STR_DUMMY_SOLDIER ]
