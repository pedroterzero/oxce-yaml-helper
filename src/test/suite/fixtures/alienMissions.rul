alienMissions:
  - type: STR_ALIEN_MISSION_WITHOUT_WAVES
    raceWeights: {}
  - type: STR_ALIEN_MISSION_WITHOUT_RACEWEIGHTS
    waves: []
  - type: STR_ALIEN_MISSION_WITHOUT_WAVE_TRAJECTORY
    raceWeights: {}
    waves:
      - ufo: STR_ABDUCTOR
        trajectory: P0
      - ufo: STR_TERROR_SHIP
  - type: STR_ALIEN_MISSION_OK
    raceWeights: {}
    waves: []
  - type: STR_ALIEN_MISSION_IN_MISSIONSCRIPT_NO_RACEWEIGHTS
    waves: []
  - type: STR_ALIEN_MISSION_IN_MISSIONSCRIPT
    waves: []
  - type: STR_ALIEN_MISSION_KEY_REFERENCE_CHECK
    waves: {}
    raceWeights:
      0:
        STR_DUMMY_RACE: 100
  - type: STR_ALIEN_MISSION_KEY_REFERENCE_TYPE_CHECK
    waves: {}
    raceWeights:
      0:
        STR_DUMMY_ITEM: 100
  # test that alienMissions.waves[].ufo reports a problem when UFO points to an non existing (this failed in the past because it was a logic field)
  - type: STR_TEST_WAVES_UFO_MISSING_TYPE
    raceWeights: {}
    waves:
      - ufo: STR_DUMMY_NON_EXISTING_REFERENCE
        trajectory: P0
  # test that alienMissions.waves[].ufo reports a problem when UFO points to an incorrect type (this failed in the past because it was a logic field)
  - type: STR_TEST_WAVES_UFO_INCORRECT_TYPE
    raceWeights: {}
    waves:
      - ufo: STR_DUMMY_ITEM
        trajectory: P0
  # test that alienMissions.regionWeights region points to a missing type
  - type: STR_TEST_WAVES_UFO_REGION_WEIGHT_MISSING_TYPE
    regionWeights:
      0: STR_DUMMY_NON_EXISTING_REFERENCE
    raceWeights: {}
    waves:
      - ufo: STR_ABDUCTOR
        trajectory: P0
  # test that alienMissions.regionWeights region points to an incorrect type
  - type: STR_TEST_WAVES_UFO_REGION_WEIGHT_INCORRECT_TYPE
    regionWeights:
      0: STR_DUMMY_ITEM
    raceWeights: {}
    waves:
      - ufo: STR_ABDUCTOR
        trajectory: P0
