  - name: STR_DUMMY_TERRAINS_CIVILIANTYPES_TEST
    civilianTypes: [STR_DUMMY_UNIT1]
  # missing type
  - name: STR_DUMMY_TERRAINS_CIVILIANTYPES_TEST_MISSING
    civilianTypes: [STR_DUMMY_NON_EXISTING_REFERENCE]
  # incorrect type
  - name: STR_DUMMY_TERRAINS_CIVILIANTYPES_TEST_INCORRECT
    civilianTypes: [STR_DUMMY_RESEARCH]

  - name: STR_DUMMY_TERRAINS_ENVIROEFFECTS_TEST
