alienRaces:
  # test that we get no diagnostic for the correct case
  - id: STR_ALIEN_RACE_MEMBER_WORKING_TEST
    members:
      - STR_DUMMY_UNIT_ALIEN_RACE_TEST
  # test for incorrect type
  - id: STR_ALIEN_RACE_MEMBER_INCORRECT_TEST
    members:
      - STR_DUMMY_ITEM
  # test for missing type
  - id: STR_ALIEN_RACE_MEMBER_MISSING_TEST
    members:
      - STR_DUMMY_NON_EXISTING_REFERENCE

  # test that we get no diagnostic for the correct case
  - id: STR_ALIEN_RACE_MEMBERS_RANDOM_WORKING_TEST1
    membersRandom:
      - [STR_DUMMY_UNIT_ALIEN_RACE_TEST]
  - id: STR_ALIEN_RACE_MEMBERS_RANDOM_WORKING_TEST2
    membersRandom:
      -
        - STR_DUMMY_UNIT_ALIEN_RACE_TEST
  # test for incorrect type
  - id: STR_ALIEN_RACE_MEMBERS_RANDOM_INCORRECT_TEST1
    membersRandom:
      - [STR_DUMMY_ITEM]
  - id: STR_ALIEN_RACE_MEMBERS_RANDOM_INCORRECT_TEST2
    membersRandom:
      -
        - STR_DUMMY_ITEM
  # test for missing type
  - id: STR_ALIEN_RACE_MEMBERS_RANDOM_MISSING_TEST
    membersRandom:
      - [STR_DUMMY_NON_EXISTING_REFERENCE]

  # test that alienRaces.retaliationMission reports a problem when it points to a missing type
  - id: STR_ALIEN_RACE_RETALIATION_MISSION_MISSING_TYPE
    retaliationMissionWeights:
      0:
        STR_DUMMY_NON_EXISTING_REFERENCE: 10
  # test that alienRaces.retaliationMission reports a problem when it points to an incorrect type
  - id: STR_ALIEN_RACE_RETALIATION_MISSION_INCORRECT_TYPE
    retaliationMissionWeights:
      0:
        STR_DUMMY_ITEM: 10
